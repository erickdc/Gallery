@model Gallery.Foto

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Foto</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.ID_FOTO)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ID_FOTO)
            @Html.ValidationMessageFor(model => model.ID_FOTO)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ID_GALERIA, "Galeria")
        </div>
        <div class="editor-field">
            @Html.DropDownList("ID_GALERIA", String.Empty)
            @Html.ValidationMessageFor(model => model.ID_GALERIA)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
